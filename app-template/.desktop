[Desktop Entry]
Type=Application
Version=*VERSION*
Name=*PURPOSELINE*
Comment=*PURPOSELINE*
# Icon path must be absolute, update on first launch
Exec=sh -c 'cd $(dirname %k) && sed -i "s,^Icon=@,Icon=$PWD,g" .desktop && ./Cobit'
Icon=@/512x512.png
Terminal=false
Categories=Finance
X-Ubuntu-Touch=true
X-Ubuntu-StageHint=SideStage
